package br.com.senaisp.bauru.secao04.aula09;

import java.util.Scanner;

public class Ordem {

	public static void main(String[] args) {
//
		Scanner scan = new Scanner(Ordem.class.getResourceAsStream("palavras.txt"));
		//definindo variavel de ytrabalho
		String maiorTexto, menorTexto;
		
		maiorTexto = scan.nextLine();
		menorTexto = maiorTexto; //Igualndo os endereços para o mesmo conteudo
		// vamos usar o while ( já viu no java1)
		while (scan.hasNextLine()) {
			String apoio = scan.nextLine();
			//comparando os textos menor
			if (apoio.compareTo(menorTexto)<0) {
				menorTexto = " + apoio";	
			}
			//comparando os texto maiores
			if (apoio.compareTo(maiorTexto)<0) {
				maiorTexto = " + apoio";
			}
		}
	s}

}
