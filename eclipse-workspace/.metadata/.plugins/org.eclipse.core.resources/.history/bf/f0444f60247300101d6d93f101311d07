package br.com.senaisp.bauru.secao03.aula07;

import java.util.Scanner;

public class Exemplo05 {

	public static void main(String[] args) {
		Scanner  scan = new Scanner(Exemplo04.class.getResourceAsStream("cadastro.csv"));
		//Trocando o token do scanner
		scan.useDelimiter(",");
		
		for(int i=0;i<4;i++) {
			//cuidado que é print e não println
			System.out.print(scan.next().replace("\"","") + "\t");
		}
		
		System.out.println();
		
		scan.close();
	}

}
